console.log("tabuada sequencia");
console.log(" ");

let t = 23 ;

console.log(t + "* 1 =" , t * 1);
console.log(t + "* 2 =" , t * 2);
console.log(t + "* 3 =" , t * 3);
console.log(t + "* 4 =" , t * 4);
console.log(t + "* 5 =" , t * 5);
console.log(t + "* 6 =" , t * 6);
console.log(t + "* 7 =" , t * 7);
console.log(t + "* 8 =" , t * 8);
console.log(t + "* 9 =" , t * 9);
console.log(t + "* 10 =" , t * 10);

t = t + 1;
console.log(" ");

console.log(t + "* 1 =" , t * 1);
console.log(t + "* 2 =" , t * 2);
console.log(t + "* 3 =" , t * 3);
console.log(t + "* 4 =" , t * 4);
console.log(t + "* 5 =" , t * 5);
console.log(t + "* 6 =" , t * 6);
console.log(t + "* 7 =" , t * 7);
console.log(t + "* 8 =" , t * 8);
console.log(t + "* 9 =" , t * 9);
console.log(t + "* 10 =" , t * 10);

t ++ ;
console.log(" ");

console.log(t + "* 1 =" , t * 1);
console.log(t + "* 2 =" , t * 2);
console.log(t + "* 3 =" , t * 3);
console.log(t + "* 4 =" , t * 4);
console.log(t + "* 5 =" , t * 5);
console.log(t + "* 6 =" , t * 6);
console.log(t + "* 7 =" , t * 7);
console.log(t + "* 8 =" , t * 8);
console.log(t + "* 9 =" , t * 9);
console.log(t + "* 10 =" , t * 10);
