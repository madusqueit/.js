console.log(soma(5,9));
function soma(V1 = 0,V2 = 0){
    return V1 + V2 ;
}

function nomeidade(nome,idade){
    console.log(`nome:${nome} - idade:${idade}`);
}
nomeidade("madu","17")
nomeidade("17","madu")

console.log(soma(9));
nomeidade("board");

console.log(multiplicacao(1358,86));
function multiplicacao(V1 = 0,V2 = 0){
    return V1 * V2 ;
}

multiplicacao(soma(3,5),soma(8,7));
multiplicacao(5);
